{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Tabby",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_icon": "icons/icon-128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}